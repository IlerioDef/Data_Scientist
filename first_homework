psql -U postgres

CREATE TABLE
Fav_films (
	id serial PRIMARY KEY,
	name VARCHAR (355) NOT NULL,
	country VARCHAR (355) NOT NULL,
	Earnings VARCHAR (355),
	Year_premiered TIMESTAMP
);

INSERT INTO
